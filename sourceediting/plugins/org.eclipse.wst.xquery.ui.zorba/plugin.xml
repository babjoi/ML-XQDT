<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.eclipse.wst.xquery.ui.handlers.ZorbaXQDocHandler"
            commandId="org.eclipse.wst.xquery.ui.commands.xqdocCommand">
         <activeWhen>
            <and>
               <count
                     value="1">
               </count>
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <test
                        forcePluginActivation="true"
                        property="org.eclipse.wst.xquery.launching.interpreterType"
                        value="org.eclipse.wst.xquery.launching.ZorbaInstallType">
                  </test>
               </iterate>
            </and>
         </activeWhen>
      </handler>
   </extension>

</plugin>
